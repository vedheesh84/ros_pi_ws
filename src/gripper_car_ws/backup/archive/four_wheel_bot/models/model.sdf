            <size>0.001 0.001 0.001</size>
          </box>
        </geometry>
      </visual>
    </link>
    
    <!-- Body link -->
    <link name='body_link'>
      <inertial>
        <pose>0 0 0.06 0 -0 0</pose>
        <mass>10</mass>
        <inertia>
          <ixx>0.312</ixx>
          <ixy>0</ixy>
          <ixz>0</ixz>
          <iyy>0.845333</iyy>
          <iyz>0</iyz>
          <izz>1.13333</izz>
        </inertia>
